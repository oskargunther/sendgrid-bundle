{
    "name": "oskargunther/sendgrid-bundle",
    "type": "library",
    "license": "MIT",
    "description": "Integrates sendgrid-php with Symfony >=2.8 with extra message profiling",
    "authors": [
        {
            "name": "Oskar Gunther",
            "email": "oskar.gunther94@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",

        "symfony/dependency-injection": ">=2.8.30",
        "symfony/event-dispatcher": ">=2.8.30",
        "symfony/config": ">=2.8.30",
        "symfony/yaml": ">=2.8.30",
        "symfony/var-dumper": ">=2.8.30",
        "sendgrid/sendgrid": "~7",
        "symfony/http-foundation": ">=2.8.30"
    },
    "autoload": {
        "psr-4": {
            "OG\\SendGridBundle\\": ""
        }
    },
    "require-dev": {
        "symfony/profiler-pack": "^1.0",
        "symfony/framework-bundle": ">=2.8.30"
    }
}
